    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.1.0",
    "fdir": "^6.5.0",
    "svelte": "^5.36.16",
    "svelte-check": "^4.2.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "@sila/client": "*",
    "@sila/core": "*"
