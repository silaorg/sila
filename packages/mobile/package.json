{
  "name": "@sila/mobile",
  "version": "1.0.0",
  "description": "Sila Mobile App - SvelteKit + Capacitor",
  "type": "module",
  "scripts": {
    "prepare": "svelte-kit sync",
    "build": "npm run build:client && npm run build:vite && npx cap sync",
    "build:vite": "vite build",
    "build:client": "npm run build -w @sila/client",
    "check": "svelte-check"
  },
  "dependencies": {
    "@sila/client": "*",
    "@sila/core": "*",
    "@capacitor/android": "^7.4.2",
    "@capacitor/ios": "^7.4.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.5",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/node": "^24.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.4.11"
  }
}
